<div id="course-rooms">
  <%= render partial: "shared/chat_room", collection: @courses, as: :chat_room %>
</div>
<div id="users">
  <%= render partial: "shared/chat_room", collection: @users, as: :chat_room %>
</div>
<div id="infinite-scrolling">
  <div class="loading" hidden>
    <i class="fa fa-spinner fa-pulse fa-2x fa-fw"></i>
    <span class="sr-only"><%= t "chats.loading" %></span>
  </div>
  <div hidden><%= paginate @users %></div>
</div>
