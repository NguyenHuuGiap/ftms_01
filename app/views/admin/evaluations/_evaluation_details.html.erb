<tr>
  <td id="evaluation-template-name" class="name">
    <%= f.text_field :name, readonly: true, size: Settings.form.min_size %>
  </td>
  <td class="point">
    <%= f.number_field :point, min: Settings.evaluations.min,
      class: "evaluation-detail-point form-control", required: true %>
  </td>
  <%= f.hidden_field :evaluation_template_id %>
</tr>
